[
	{
		"name":"jenkins",
		"image":"jenkins",
		"run":"docker run --name jenkins -d --restart=always -p 8080:8080 -v /var/jenkins jenkins"
	},
	{
		"name":"gogs",
		"image":"codeskyblue/docker-gogs",
		"run":"docker run --name=gogs -d --restart=always -p 3000:3000 -v /var/gogs:/data codeskyblue/docker-gogs"
	},
	{
		"name":"dockerboard",
		"image":"dockerboard",
		"run":"docker run -d -p 8001:8001 --restart=always --name dockerboard -v /var/run/docker.sock:/var/run/docker.sock dockerboard/dockerboard"
	},
	{
		"name":"taskboard",
		"image":"mirweb/taskboard",
		"run":"docker run --name taskboard -d --restart=always -p 4000:80 mirweb/taskboard"
	},
	{
		"name":"registry",
		"image":"registry",
		"run":"docker run --name registry -d --restart=always -p 5000:5000 registry"
	}
]
